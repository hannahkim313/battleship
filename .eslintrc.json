{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "comma-dangle": ["error", "always-multiline"]
    }
}
